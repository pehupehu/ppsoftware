<!DOCTYPE html>
<html lang="{{ app.request.locale|split('_')[0] }}">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="stylesheet" href="{{ asset('build/bootstrap-4.1.3-dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('build/open-iconic/font/css/open-iconic-bootstrap.css') }}">
        <link rel="stylesheet" href="{{ asset('build/jquery-ui-1.12.1/jquery-ui.min.css') }}">
        <link rel="stylesheet" href="{{ asset('build/jquery-ui-1.12.1/jquery-ui.theme.min.css') }}">
        <link rel="stylesheet" href="{{ asset('build/flags/flags.min.css') }}">
        <link rel="stylesheet" href="{{ asset('build/css/main.css') }}">
        {% block stylesheets %}{% endblock %}
        <script src="{{ asset('build/jquery-3.3.1.min.js') }}"></script>
        <script src="{{ asset('build/jquery-ui-1.12.1/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('build/popper.min.js') }}"></script>
        <script src="{{ asset('build/bootstrap-4.1.3-dist/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
        <script src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
        <script src="{{ url('bazinga_jstranslation_js') }}"></script>
        <script src="{{ asset('js/ppbox.js') }}"></script>
    </head>
    <body>
        {% block navbar %}{% endblock navbar %}
        {% block body %}{% endblock %}
        <script src="{{ asset('js/ppcheck.js') }}"></script>
        <script src="{{ asset('js/app.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
